{
  "name": "turborepo-saas-boilerplate-code",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "start": "turbo start",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:migrate": "cd packages/prisma-db && npx prisma migrate dev --name init",
    "db:generate": "cd packages/prisma-db && npx prisma generate",
    "db:seed": "cd packages/prisma-db && npx prisma db seed",
    "docs:deploy": "cd apps/docs && npm run deploy-s3",
    "docs:serve": "cd apps/docs && npm run serve -- --host 0.0.0.0 --no-open"
  },
  "devDependencies": {
    "@repo/auth": "*",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "prettier": "^3.4.2",
    "sonarqube-scanner": "^4.2.6",
    "turbo": "^2.3.3",
    "typescript": "5.5.4",
    "vitest": "^3.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@sparticuz/chromium": "^132.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dodopayments": "^0.19.0",
    "lucide-react": "^0.474.0",
    "nextjs-toploader": "^3.7.15",
    "puppeteer-core": "^24.1.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
